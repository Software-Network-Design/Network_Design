//登陆响应返回示例
{
    "send": "server",
    "receive": "u123",
    "type": 2,
    "info": {
        "success": "登陆成功",//或"无此用户"或"密码错误"
        "strangers": {
            "strangers_num": 0
        },
        "friends": {
            "friends_num": 2,
            // 根据friends_num变化
            "friend0": {
                "user_id": "u234",
                "user_name": "xyz"
            },
            "friend1":{
                // ...
            },
            // 同上
            "strangers_num": 1,
            "stranger0": {
                // ...
            }
        }
    }
}
// 好友上线提醒
{
    "send": "server",
    // 被提醒人
    "receive": {
        "user_id": "u123",
        "user_name": "zyx"
    },
    "type": 8,
    // 好友id及name
    "info": {
        "user_id": "u234",
        "user_name": "xyz"
    }
}